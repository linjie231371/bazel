package(default_visibility = ["//visibility:public"])

licenses(["restricted"])  # GNU GPL v2 with Classpath exception

filegroup(
    name = "srcs",
    srcs = glob(["**"]),
)

java_import(
    name = "javac",
    jars = ["javac.jar"],
)

filegroup(
    name = "javac_jar",
    srcs = ["javac.jar"],
)
